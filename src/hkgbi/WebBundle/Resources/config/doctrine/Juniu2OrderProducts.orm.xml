<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="hkgbi\WebBundle\Entity\Juniu2OrderProducts" table="juniu2_order_products">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="orderid" type="integer" column="orderid" nullable="false"/>
    <field name="proid" type="integer" column="proid" nullable="false"/>
    <field name="title" type="string" column="title" length="255" nullable="false"/>
    <field name="amount" type="smallint" column="amount" nullable="false"/>
    <field name="price" type="float" column="price" precision="10" scale="0" nullable="false"/>
    <field name="priceCurrency" type="string" column="price_currency" length="5" nullable="false"/>
    <field name="thumbId" type="integer" column="thumb_id" nullable="false"/>
  </entity>
</doctrine-mapping>
