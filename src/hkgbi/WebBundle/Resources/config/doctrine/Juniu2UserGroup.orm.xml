<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="hkgbi\WebBundle\Entity\Juniu2UserGroup" table="juniu2_user_group">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="groupType" type="string" column="group_type" nullable="false"/>
    <field name="title" type="string" column="title" length="100" nullable="false"/>
    <field name="popedomPost" type="text" column="popedom_post" nullable="false"/>
    <field name="popedomReply" type="text" column="popedom_reply" nullable="false"/>
    <field name="popedomRead" type="text" column="popedom_read" nullable="false"/>
    <field name="postCert" type="boolean" column="post_cert" nullable="false"/>
    <field name="replyCert" type="boolean" column="reply_cert" nullable="false"/>
    <field name="ifsystem" type="boolean" column="ifsystem" nullable="false"/>
    <field name="ifdefault" type="boolean" column="ifdefault" nullable="false"/>
    <field name="ifshow" type="boolean" column="ifshow" nullable="false"/>
  </entity>
</doctrine-mapping>
