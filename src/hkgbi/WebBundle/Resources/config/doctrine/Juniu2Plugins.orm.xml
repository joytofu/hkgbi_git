<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="hkgbi\WebBundle\Entity\Juniu2Plugins" table="juniu2_plugins">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="langid" type="string" column="langid" length="10" nullable="false"/>
    <field name="identifier" type="string" column="identifier" length="100" nullable="false"/>
    <field name="status" type="boolean" column="status" nullable="false"/>
    <field name="title" type="string" column="title" length="100" nullable="false"/>
    <field name="note" type="string" column="note" length="255" nullable="false"/>
    <field name="uninstallSql" type="text" column="uninstall_sql" nullable="false"/>
    <field name="installSql" type="text" column="install_sql" nullable="false"/>
    <field name="hooks" type="text" column="hooks" nullable="false"/>
    <field name="taxis" type="boolean" column="taxis" nullable="false"/>
    <field name="version" type="string" column="version" length="20" nullable="false"/>
    <field name="author" type="string" column="author" length="100" nullable="false"/>
    <field name="postdate" type="integer" column="postdate" nullable="false"/>
    <field name="ext" type="text" column="ext" nullable="false"/>
  </entity>
</doctrine-mapping>
