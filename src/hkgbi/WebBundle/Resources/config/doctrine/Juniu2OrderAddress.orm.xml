<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="hkgbi\WebBundle\Entity\Juniu2OrderAddress" table="juniu2_order_address">
    <id name="orderId" type="integer" column="order_id"/>
    <id name="addressType" type="string" column="address_type"/>
    <field name="fullname" type="string" column="fullname" length="100" nullable="false"/>
    <field name="tel" type="string" column="tel" length="30" nullable="false"/>
    <field name="email" type="string" column="email" length="70" nullable="false"/>
    <field name="zipcode" type="string" column="zipcode" length="20" nullable="false"/>
    <field name="country" type="string" column="country" length="100" nullable="false"/>
    <field name="address" type="text" column="address" nullable="false"/>
    <field name="note" type="text" column="note" nullable="false"/>
  </entity>
</doctrine-mapping>
