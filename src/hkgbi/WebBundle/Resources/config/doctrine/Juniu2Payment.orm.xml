<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="hkgbi\WebBundle\Entity\Juniu2Payment" table="juniu2_payment">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="code" type="string" column="code" length="100" nullable="false"/>
    <field name="title" type="string" column="title" length="255" nullable="false"/>
    <field name="status" type="boolean" column="status" nullable="false"/>
    <field name="taxis" type="boolean" column="taxis" nullable="false"/>
    <field name="nextAct" type="string" column="next_act" length="100" nullable="false"/>
    <field name="note" type="text" column="note" nullable="false"/>
    <field name="langid" type="string" column="langid" length="5" nullable="false"/>
  </entity>
</doctrine-mapping>
